{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "Rufus",
    "fileTypes": [
        "rfs"
    ],
    "scopeName": "source.rufus",
    "patterns": [
        {
            "comment": "boxed slice literal",
            "begin": "(<)(\\[)",
            "beginCaptures": {
                "1": {
                    "name": "punctuation.brackets.angle.rufus"
                },
                "2": {
                    "name": "punctuation.brackets.square.rufus"
                }
            },
            "end": ">",
            "endCaptures": {
                "0": {
                    "name": "punctuation.brackets.angle.rufus"
                }
            },
            "patterns": [
                {
                    "include": "#block-comments"
                },
                {
                    "include": "#comments"
                },
                {
                    "include": "#gtypes"
                },
                {
                    "include": "#lvariables"
                },
                {
                    "include": "#lifetimes"
                },
                {
                    "include": "#punctuation"
                },
                {
                    "include": "#types"
                }
            ]
        },
        {
            "comment": "macro type metavariables",
            "name": "meta.macro.metavariable.type.rufus",
            "match": "(\\$)((crate)|([A-Z][A-Za-z0-9_]*))((:)(block|expr|ident|item|lifetime|literal|meta|path?|stmt|tt|ty|vis))?",
            "captures": {
                "1": {
                    "name": "keyword.operator.macro.dollar.rufus"
                },
                "3": {
                    "name": "keyword.other.crate.rufus"
                },
                "4": {
                    "name": "entity.name.type.metavariable.rufus"
                },
                "6": {
                    "name": "keyword.operator.key-value.rufus"
                },
                "7": {
                    "name": "variable.other.metavariable.specifier.rufus"
                }
            },
            "patterns": [
                {
                    "include": "#keywords"
                }
            ]
        },
        {
            "comment": "macro metavariables",
            "name": "meta.macro.metavariable.rufus",
            "match": "(\\$)([a-z][A-Za-z0-9_]*)((:)(block|expr|ident|item|lifetime|literal|meta|path?|stmt|tt|ty|vis))?",
            "captures": {
                "1": {
                    "name": "keyword.operator.macro.dollar.rufus"
                },
                "2": {
                    "name": "variable.other.metavariable.name.rufus"
                },
                "4": {
                    "name": "keyword.operator.key-value.rufus"
                },
                "5": {
                    "name": "variable.other.metavariable.specifier.rufus"
                }
            },
            "patterns": [
                {
                    "include": "#keywords"
                }
            ]
        },
        {
            "comment": "macro rules",
            "name": "meta.macro.rules.rufus",
            "match": "\\b(macro_rules!)\\s+(([a-z0-9_]+)|([A-Z][a-z0-9_]*))\\s+(\\{)",
            "captures": {
                "1": {
                    "name": "entity.name.function.macro.rules.rufus"
                },
                "3": {
                    "name": "entity.name.function.macro.rufus"
                },
                "4": {
                    "name": "entity.name.type.macro.rufus"
                },
                "5": {
                    "name": "punctuation.brackets.curly.rufus"
                }
            }
        },
        {
            "comment": "attributes",
            "name": "meta.attribute.rufus",
            "begin": "(#)(\\!?)(\\[)",
            "beginCaptures": {
                "1": {
                    "name": "punctuation.definition.attribute.rufus"
                },
                "2": {
                    "name": "keyword.operator.attribute.inner.rufus"
                },
                "3": {
                    "name": "punctuation.brackets.attribute.rufus"
                }
            },
            "end": "\\]",
            "endCaptures": {
                "0": {
                    "name": "punctuation.brackets.attribute.rufus"
                }
            },
            "patterns": [
                {
                    "include": "#block-comments"
                },
                {
                    "include": "#comments"
                },
                {
                    "include": "#keywords"
                },
                {
                    "include": "#punctuation"
                },
                {
                    "include": "#strings"
                },
                {
                    "include": "#gtypes"
                },
                {
                    "include": "#types"
                }
            ]
        },
        {
            "comment": "modules",
            "match": "(mod)\\s+((?:r#(?!crate|[Ss]elf|super))?[a-z][A-Za-z0-9_]*)",
            "captures": {
                "1": {
                    "name": "storage.type.rufus"
                },
                "2": {
                    "name": "entity.name.module.rufus"
                }
            }
        },
        {
            "comment": "external crate imports",
            "name": "meta.import.rufus",
            "begin": "\\b(extern)\\s+(crate)",
            "beginCaptures": {
                "1": {
                    "name": "storage.type.rufus"
                },
                "2": {
                    "name": "keyword.other.crate.rufus"
                }
            },
            "end": ";",
            "endCaptures": {
                "0": {
                    "name": "punctuation.semi.rufus"
                }
            },
            "patterns": [
                {
                    "include": "#block-comments"
                },
                {
                    "include": "#comments"
                },
                {
                    "include": "#keywords"
                },
                {
                    "include": "#punctuation"
                }
            ]
        },
        {
            "comment": "use statements",
            "name": "meta.use.rufus",
            "begin": "\\b(use)\\s",
            "beginCaptures": {
                "1": {
                    "name": "keyword.other.rufus"
                }
            },
            "end": ";",
            "endCaptures": {
                "0": {
                    "name": "punctuation.semi.rufus"
                }
            },
            "patterns": [
                {
                    "include": "#block-comments"
                },
                {
                    "include": "#comments"
                },
                {
                    "include": "#keywords"
                },
                {
                    "include": "#namespaces"
                },
                {
                    "include": "#punctuation"
                },
                {
                    "include": "#types"
                },
                {
                    "include": "#lvariables"
                }
            ]
        },
        {
            "include": "#block-comments"
        },
        {
            "include": "#comments"
        },
        {
            "include": "#lvariables"
        },
        {
            "include": "#constants"
        },
        {
            "include": "#gtypes"
        },
        {
            "include": "#functions"
        },
        {
            "include": "#types"
        },
        {
            "include": "#keywords"
        },
        {
            "include": "#lifetimes"
        },
        {
            "include": "#macros"
        },
        {
            "include": "#namespaces"
        },
        {
            "include": "#punctuation"
        },
        {
            "include": "#strings"
        },
        {
            "include": "#variables"
        }
    ],
    "repository": {
        "comments": {
            "patterns": [
                {
                    "comment": "documentation comments",
                    "name": "comment.line.documentation.rufus",
                    "match": "^\\s*///.*"
                },
                {
                    "comment": "line comments",
                    "name": "comment.line.double-slash.rufus",
                    "match": "\\s*//.*"
                }
            ]
        },
        "block-comments": {
            "patterns": [
                {
                    "comment": "block comments",
                    "name": "comment.block.rufus",
                    "begin": "/\\*(?!\\*)",
                    "end": "\\*/",
                    "patterns": [
                        {
                            "include": "#block-comments"
                        }
                    ]
                },
                {
                    "comment": "block documentation comments",
                    "name": "comment.block.documentation.rufus",
                    "begin": "/\\*\\*",
                    "end": "\\*/",
                    "patterns": [
                        {
                            "include": "#block-comments"
                        }
                    ]
                }
            ]
        },
        "constants": {
            "patterns": [
                {
                    "comment": "ALL CAPS constants",
                    "name": "constant.other.caps.rufus",
                    "match": "\\b[A-Z]{2}[A-Z0-9_]*\\b"
                },
                {
                    "comment": "constant declarations",
                    "match": "\\b(const)\\s+([A-Z][A-Za-z0-9_]*)\\b",
                    "captures": {
                        "1": {
                            "name": "storage.type.rufus"
                        },
                        "2": {
                            "name": "constant.other.caps.rufus"
                        }
                    }
                },
                {
                    "comment": "decimal integers and floats",
                    "name": "constant.numeric.decimal.rufus",
                    "match": "\\b\\d[\\d_]*(\\.?)[\\d_]*(?:(E)([+-])([\\d_]+))?(f32|f64|i128|i16|i32|i64|i8|isize|u128|u16|u32|u64|u8|usize)?\\b",
                    "captures": {
                        "1": {
                            "name": "punctuation.separator.dot.decimal.rufus"
                        },
                        "2": {
                            "name": "keyword.operator.exponent.rufus"
                        },
                        "3": {
                            "name": "keyword.operator.exponent.sign.rufus"
                        },
                        "4": {
                            "name": "constant.numeric.decimal.exponent.mantissa.rufus"
                        },
                        "5": {
                            "name": "entity.name.type.numeric.rufus"
                        }
                    }
                },
                {
                    "comment": "hexadecimal integers",
                    "name": "constant.numeric.hex.rufus",
                    "match": "\\b0x[\\da-fA-F_]+(i128|i16|i32|i64|i8|isize|u128|u16|u32|u64|u8|usize)?\\b",
                    "captures": {
                        "1": {
                            "name": "entity.name.type.numeric.rufus"
                        }
                    }
                },
                {
                    "comment": "octal integers",
                    "name": "constant.numeric.oct.rufus",
                    "match": "\\b0o[0-7_]+(i128|i16|i32|i64|i8|isize|u128|u16|u32|u64|u8|usize)?\\b",
                    "captures": {
                        "1": {
                            "name": "entity.name.type.numeric.rufus"
                        }
                    }
                },
                {
                    "comment": "binary integers",
                    "name": "constant.numeric.bin.rufus",
                    "match": "\\b0b[01_]+(i128|i16|i32|i64|i8|isize|u128|u16|u32|u64|u8|usize)?\\b",
                    "captures": {
                        "1": {
                            "name": "entity.name.type.numeric.rufus"
                        }
                    }
                },
                {
                    "comment": "booleans",
                    "name": "constant.language.bool.rufus",
                    "match": "\\btrue|false\\b"
                }
            ]
        },
        "escapes": {
            "comment": "escapes: ASCII, byte, Unicode, quote, regex",
            "name": "constant.character.escape.rufus",
            "match": "(\\\\)(?:(?:(x[0-7][0-7a-fA-F])|(u(\\{)[\\da-fA-F]{4,6}(\\}))|.))",
            "captures": {
                "1": {
                    "name": "constant.character.escape.backslash.rufus"
                },
                "2": {
                    "name": "constant.character.escape.bit.rufus"
                },
                "3": {
                    "name": "constant.character.escape.unicode.rufus"
                },
                "4": {
                    "name": "constant.character.escape.unicode.punctuation.rufus"
                },
                "5": {
                    "name": "constant.character.escape.unicode.punctuation.rufus"
                }
            }
        },
        "functions": {
            "patterns": [
                {
                    "comment": "pub as a function",
                    "match": "\\b(pub)(\\()",
                    "captures": {
                        "1": {
                            "name": "keyword.other.rufus"
                        },
                        "2": {
                            "name": "punctuation.brackets.round.rufus"
                        }
                    }
                },
                {
                    "comment": "function definition",
                    "name": "meta.function.definition.rufus",
                    "begin": "\\b(fn)\\s+((?:r#(?!crate|[Ss]elf|super))?[A-Za-z0-9_]+)((\\()|(<))",
                    "beginCaptures": {
                        "1": {
                            "name": "keyword.other.fn.rufus"
                        },
                        "2": {
                            "name": "entity.name.function.rufus"
                        },
                        "4": {
                            "name": "punctuation.brackets.round.rufus"
                        },
                        "5": {
                            "name": "punctuation.brackets.angle.rufus"
                        }
                    },
                    "end": "\\{|;",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.brackets.curly.rufus"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#block-comments"
                        },
                        {
                            "include": "#comments"
                        },
                        {
                            "include": "#keywords"
                        },
                        {
                            "include": "#lvariables"
                        },
                        {
                            "include": "#constants"
                        },
                        {
                            "include": "#gtypes"
                        },
                        {
                            "include": "#functions"
                        },
                        {
                            "include": "#lifetimes"
                        },
                        {
                            "include": "#macros"
                        },
                        {
                            "include": "#namespaces"
                        },
                        {
                            "include": "#punctuation"
                        },
                        {
                            "include": "#strings"
                        },
                        {
                            "include": "#types"
                        },
                        {
                            "include": "#variables"
                        }
                    ]
                },
                {
                    "comment": "function/method calls, chaining",
                    "name": "meta.function.call.rufus",
                    "begin": "((?:r#(?!crate|[Ss]elf|super))?[A-Za-z0-9_]+)(\\()",
                    "beginCaptures": {
                        "1": {
                            "name": "entity.name.function.rufus"
                        },
                        "2": {
                            "name": "punctuation.brackets.round.rufus"
                        }
                    },
                    "end": "\\)",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.brackets.round.rufus"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#block-comments"
                        },
                        {
                            "include": "#comments"
                        },
                        {
                            "include": "#keywords"
                        },
                        {
                            "include": "#lvariables"
                        },
                        {
                            "include": "#constants"
                        },
                        {
                            "include": "#gtypes"
                        },
                        {
                            "include": "#functions"
                        },
                        {
                            "include": "#lifetimes"
                        },
                        {
                            "include": "#macros"
                        },
                        {
                            "include": "#namespaces"
                        },
                        {
                            "include": "#punctuation"
                        },
                        {
                            "include": "#strings"
                        },
                        {
                            "include": "#types"
                        },
                        {
                            "include": "#variables"
                        }
                    ]
                },
                {
                    "comment": "function/method calls with turbofish",
                    "name": "meta.function.call.rufus",
                    "begin": "((?:r#(?!crate|[Ss]elf|super))?[A-Za-z0-9_]+)(?=::<.*>\\()",
                    "beginCaptures": {
                        "1": {
                            "name": "entity.name.function.rufus"
                        }
                    },
                    "end": "\\)",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.brackets.round.rufus"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#block-comments"
                        },
                        {
                            "include": "#comments"
                        },
                        {
                            "include": "#keywords"
                        },
                        {
                            "include": "#lvariables"
                        },
                        {
                            "include": "#constants"
                        },
                        {
                            "include": "#gtypes"
                        },
                        {
                            "include": "#functions"
                        },
                        {
                            "include": "#lifetimes"
                        },
                        {
                            "include": "#macros"
                        },
                        {
                            "include": "#namespaces"
                        },
                        {
                            "include": "#punctuation"
                        },
                        {
                            "include": "#strings"
                        },
                        {
                            "include": "#types"
                        },
                        {
                            "include": "#variables"
                        }
                    ]
                }
            ]
        },
        "keywords": {
            "patterns": [
                {
                    "comment": "control flow keywords",
                    "name": "keyword.control.rufus",
                    "match": "\\b(await|break|continue|do|else|for|if|loop|match|return|try|while|yield)\\b"
                },
                {
                    "comment": "storage keywords",
                    "name": "storage.type.rufus",
                    "match": "\\b(const|enum|extern|let|macro|mod|struct|trait|type)\\b"
                },
                {
                    "comment": "storage modifiers",
                    "name": "storage.modifier.rufus",
                    "match": "\\b(abstract|static)\\b"
                },
                {
                    "comment": "other keywords",
                    "name": "keyword.other.rufus",
                    "match": "\\b(as|async|become|box|dyn|move|final|impl|in|override|priv|pub|ref|typeof|union|unsafe|unsized|use|virtual|where)\\b"
                },
                {
                    "comment": "fn",
                    "name": "keyword.other.fn.rufus",
                    "match": "\\bfn\\b"
                },
                {
                    "comment": "crate",
                    "name": "keyword.other.crate.rufus",
                    "match": "\\bcrate\\b"
                },
                {
                    "comment": "mut",
                    "name": "storage.modifier.mut.rufus",
                    "match": "\\bmut\\b"
                },
                {
                    "comment": "logical operators",
                    "name": "keyword.operator.logical.rufus",
                    "match": "(\\^|\\||\\|\\||&&|<<|>>|!)(?!=)"
                },
                {
                    "comment": "logical AND, borrow references",
                    "name": "keyword.operator.borrow.and.rufus",
                    "match": "&(?![&=])"
                },
                {
                    "comment": "assignment operators",
                    "name": "keyword.operator.assignment.rufus",
                    "match": "(\\+=|-=|\\*=|/=|%=|\\^=|&=|\\|=|<<=|>>=)"
                },
                {
                    "comment": "single equal",
                    "name": "keyword.operator.assignment.equal.rufus",
                    "match": "(?<![<>])=(?!=|>)"
                },
                {
                    "comment": "comparison operators",
                    "name": "keyword.operator.comparison.rufus",
                    "match": "(=(=)?(?!>)|!=|<=|(?<!=)>=)"
                },
                {
                    "comment": "math operators",
                    "name": "keyword.operator.math.rufus",
                    "match": "(([+%]|(\\*(?!\\w)))(?!=))|(-(?!>))|(/(?!/))"
                },
                {
                    "comment": "less than, greater than (special case)",
                    "match": "(?:\\b|(?:(\\))|(\\])|(\\})))[ \\t]+([<>])[ \\t]+(?:\\b|(?:(\\()|(\\[)|(\\{)))",
                    "captures": {
                        "1": {
                            "name": "punctuation.brackets.round.rufus"
                        },
                        "2": {
                            "name": "punctuation.brackets.square.rufus"
                        },
                        "3": {
                            "name": "punctuation.brackets.curly.rufus"
                        },
                        "4": {
                            "name": "keyword.operator.comparison.rufus"
                        },
                        "5": {
                            "name": "punctuation.brackets.round.rufus"
                        },
                        "6": {
                            "name": "punctuation.brackets.square.rufus"
                        },
                        "7": {
                            "name": "punctuation.brackets.curly.rufus"
                        }
                    }
                },
                {
                    "comment": "namespace operator",
                    "name": "keyword.operator.namespace.rufus",
                    "match": "::"
                },
                {
                    "comment": "dereference asterisk",
                    "match": "(\\*)(?=\\w+)",
                    "captures": {
                        "1": {
                            "name": "keyword.operator.dereference.rufus"
                        }
                    }
                },
                {
                    "comment": "subpattern binding",
                    "name": "keyword.operator.subpattern.rufus",
                    "match": "@"
                },
                {
                    "comment": "dot access",
                    "name": "keyword.operator.access.dot.rufus",
                    "match": "\\.(?!\\.)"
                },
                {
                    "comment": "ranges, range patterns",
                    "name": "keyword.operator.range.rufus",
                    "match": "\\.{2}(=|\\.)?"
                },
                {
                    "comment": "colon",
                    "name": "keyword.operator.key-value.rufus",
                    "match": ":(?!:)"
                },
                {
                    "comment": "dashrocket, skinny arrow",
                    "name": "keyword.operator.arrow.skinny.rufus",
                    "match": "->"
                },
                {
                    "comment": "hashrocket, fat arrow",
                    "name": "keyword.operator.arrow.fat.rufus",
                    "match": "=>"
                },
                {
                    "comment": "dollar macros",
                    "name": "keyword.operator.macro.dollar.rufus",
                    "match": "\\$"
                },
                {
                    "comment": "question mark operator, questionably sized, macro kleene matcher",
                    "name": "keyword.operator.question.rufus",
                    "match": "\\?"
                }
            ]
        },
        "interpolations": {
            "comment": "curly brace interpolations",
            "name": "meta.interpolation.rufus",
            "match": "({)[^\"{}]*(})",
            "captures": {
                "1": {
                    "name": "punctuation.definition.interpolation.rufus"
                },
                "2": {
                    "name": "punctuation.definition.interpolation.rufus"
                }
            }
        },
        "lifetimes": {
            "patterns": [
                {
                    "comment": "named lifetime parameters",
                    "match": "(['])([a-zA-Z_][0-9a-zA-Z_]*)(?!['])\\b",
                    "captures": {
                        "1": {
                            "name": "punctuation.definition.lifetime.rufus"
                        },
                        "2": {
                            "name": "entity.name.type.lifetime.rufus"
                        }
                    }
                },
                {
                    "comment": "borrowing references to named lifetimes",
                    "match": "(\\&)(['])([a-zA-Z_][0-9a-zA-Z_]*)(?!['])\\b",
                    "captures": {
                        "1": {
                            "name": "keyword.operator.borrow.rufus"
                        },
                        "2": {
                            "name": "punctuation.definition.lifetime.rufus"
                        },
                        "3": {
                            "name": "entity.name.type.lifetime.rufus"
                        }
                    }
                }
            ]
        },
        "macros": {
            "patterns": [
                {
                    "comment": "macros",
                    "name": "meta.macro.rufus",
                    "match": "(([a-z_][A-Za-z0-9_]*!)|([A-Z_][A-Za-z0-9_]*!))",
                    "captures": {
                        "2": {
                            "name": "entity.name.function.macro.rufus"
                        },
                        "3": {
                            "name": "entity.name.type.macro.rufus"
                        }
                    }
                }
            ]
        },
        "namespaces": {
            "patterns": [
                {
                    "comment": "namespace (non-type, non-function path segment)",
                    "match": "(?<![A-Za-z0-9_])([a-z0-9_]+)((?<!super|self)::)",
                    "captures": {
                        "1": {
                            "name": "entity.name.namespace.rufus"
                        },
                        "2": {
                            "name": "keyword.operator.namespace.rufus"
                        }
                    }
                }
            ]
        },
        "types": {
            "patterns": [
                {
                    "comment": "numeric types",
                    "match": "(?<![A-Za-z])(f32|f64|i128|i16|i32|i64|i8|isize|u128|u16|u32|u64|u8|usize)\\b",
                    "captures": {
                        "1": {
                            "name": "entity.name.type.numeric.rufus"
                        }
                    }
                },
                {
                    "comment": "parameterized types",
                    "begin": "\\b([A-Z][A-Za-z0-9]*)(<)",
                    "beginCaptures": {
                        "1": {
                            "name": "entity.name.type.rufus"
                        },
                        "2": {
                            "name": "punctuation.brackets.angle.rufus"
                        }
                    },
                    "end": ">",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.brackets.angle.rufus"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#block-comments"
                        },
                        {
                            "include": "#comments"
                        },
                        {
                            "include": "#keywords"
                        },
                        {
                            "include": "#lvariables"
                        },
                        {
                            "include": "#lifetimes"
                        },
                        {
                            "include": "#punctuation"
                        },
                        {
                            "include": "#types"
                        },
                        {
                            "include": "#variables"
                        }
                    ]
                },
                {
                    "comment": "primitive types",
                    "name": "entity.name.type.primitive.rufus",
                    "match": "\\b(bool|char|str)\\b"
                },
                {
                    "comment": "trait declarations",
                    "match": "\\b(trait)\\s+([A-Z][A-Za-z0-9]*)\\b",
                    "captures": {
                        "1": {
                            "name": "storage.type.rufus"
                        },
                        "2": {
                            "name": "entity.name.type.trait.rufus"
                        }
                    }
                },
                {
                    "comment": "struct declarations",
                    "match": "\\b(struct)\\s+([A-Z][A-Za-z0-9]*)\\b",
                    "captures": {
                        "1": {
                            "name": "storage.type.rufus"
                        },
                        "2": {
                            "name": "entity.name.type.struct.rufus"
                        }
                    }
                },
                {
                    "comment": "enum declarations",
                    "match": "\\b(enum)\\s+([A-Z][A-Za-z0-9_]*)\\b",
                    "captures": {
                        "1": {
                            "name": "storage.type.rufus"
                        },
                        "2": {
                            "name": "entity.name.type.enum.rufus"
                        }
                    }
                },
                {
                    "comment": "type declarations",
                    "match": "\\b(type)\\s+([A-Z][A-Za-z0-9_]*)\\b",
                    "captures": {
                        "1": {
                            "name": "storage.type.rufus"
                        },
                        "2": {
                            "name": "entity.name.type.declaration.rufus"
                        }
                    }
                },
                {
                    "comment": "types",
                    "name": "entity.name.type.rufus",
                    "match": "\\b[A-Z][A-Za-z0-9]*\\b(?!!)"
                }
            ]
        },
        "gtypes": {
            "patterns": [
                {
                    "comment": "option types",
                    "name": "entity.name.type.option.rufus",
                    "match": "\\b(Some|None)\\b"
                },
                {
                    "comment": "result types",
                    "name": "entity.name.type.result.rufus",
                    "match": "\\b(Ok|Err)\\b"
                }
            ]
        },
        "punctuation": {
            "patterns": [
                {
                    "comment": "comma",
                    "name": "punctuation.comma.rufus",
                    "match": ","
                },
                {
                    "comment": "curly braces",
                    "name": "punctuation.brackets.curly.rufus",
                    "match": "[{}]"
                },
                {
                    "comment": "parentheses, round brackets",
                    "name": "punctuation.brackets.round.rufus",
                    "match": "[()]"
                },
                {
                    "comment": "semicolon",
                    "name": "punctuation.semi.rufus",
                    "match": ";"
                },
                {
                    "comment": "square brackets",
                    "name": "punctuation.brackets.square.rufus",
                    "match": "[\\[\\]]"
                },
                {
                    "comment": "angle brackets",
                    "name": "punctuation.brackets.angle.rufus",
                    "match": "(?<!=)[<>]"
                }
            ]
        },
        "strings": {
            "patterns": [
                {
                    "comment": "double-quoted strings and byte strings",
                    "name": "string.quoted.double.rufus",
                    "begin": "(b?)(\")",
                    "beginCaptures": {
                        "1": {
                            "name": "string.quoted.byte.raw.rufus"
                        },
                        "2": {
                            "name": "punctuation.definition.string.rufus"
                        }
                    },
                    "end": "\"",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.definition.string.rufus"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#escapes"
                        },
                        {
                            "include": "#interpolations"
                        }
                    ]
                },
                {
                    "comment": "double-quoted raw strings and raw byte strings",
                    "name": "string.quoted.double.rufus",
                    "begin": "(b?r)(#*)(\")",
                    "beginCaptures": {
                        "1": {
                            "name": "string.quoted.byte.raw.rufus"
                        },
                        "2": {
                            "name": "punctuation.definition.string.raw.rufus"
                        },
                        "3": {
                            "name": "punctuation.definition.string.rufus"
                        }
                    },
                    "end": "(\")(\\2)",
                    "endCaptures": {
                        "1": {
                            "name": "punctuation.definition.string.rufus"
                        },
                        "2": {
                            "name": "punctuation.definition.string.raw.rufus"
                        }
                    }
                },
                {
                    "comment": "characters and bytes",
                    "name": "string.quoted.single.char.rufus",
                    "begin": "(b)?(')",
                    "beginCaptures": {
                        "1": {
                            "name": "string.quoted.byte.raw.rufus"
                        },
                        "2": {
                            "name": "punctuation.definition.char.rufus"
                        }
                    },
                    "end": "'",
                    "endCaptures": {
                        "0": {
                            "name": "punctuation.definition.char.rufus"
                        }
                    },
                    "patterns": [
                        {
                            "include": "#escapes"
                        }
                    ]
                }
            ]
        },
        "lvariables": {
            "patterns": [
                {
                    "comment": "self",
                    "name": "variable.language.self.rufus",
                    "match": "\\b[Ss]elf\\b"
                },
                {
                    "comment": "super",
                    "name": "variable.language.super.rufus",
                    "match": "\\bsuper\\b"
                }
            ]
        },
        "variables": {
            "patterns": [
                {
                    "comment": "variables",
                    "name": "variable.other.rufus",
                    "match": "\\b(?<!(?<!\\.)\\.)(?:r#(?!(crate|[Ss]elf|super)))?[a-z0-9_]+\\b"
                }
            ]
        }
    }
}
